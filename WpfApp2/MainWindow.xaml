<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        xmlns:views="clr-namespace:WpfApp2.Views"
        mc:Ignorable="d"
        DataContext="{Binding Path=MainWindowModel, Source={StaticResource Locator}}"
        Title="{Binding Title}" Height="400" Width="700">
    <Grid>
        <Button Content="Привет!" Width="200" Height="100"
                Command="{Binding Path=ShowDialogCommand}" CommandParameter="Привет всем!"/>
        <views:UserControl1/>
    </Grid>
</Window>
